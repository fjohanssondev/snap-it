{
  "expo": {
    "name": "snapit",
    "slug": "snapit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "scheme": "snapit",
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@/components": [
          "components"
        ]
      }
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      }
    },
    "ios": {
      "bundleIdentifier": "com.fjohansson.snapit"
    }
  }
}
